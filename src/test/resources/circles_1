bounds 250 250 center invert_y

# Center point
color 128 128 128; point  0 0 3

# Circles
for1 10 10 130 "circle 0 0 %d"

# The above for will unroll like this:

#circle 0 0 120
#circle 0 0 110
#circle 0 0 100
#circle 0 0 90
#circle 0 0 80
#circle 0 0 70
#circle 0 0 60
#circle 0 0 50
#circle 0 0 40
#circle 0 0 30
#circle 0 0 20
#circle 0 0 10
